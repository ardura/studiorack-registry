{
  "name": "studiorack-registry",
  "version": "0.0.1",
  "description": "Audio plugin registry with searchable plugin list, metadata and download urls",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp ./src/registry.json ./dist",
    "dev": "npx tsx src/index.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "echo 'No tests were run'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiorack/studiorack-registry.git"
  },
  "keywords": [
    "audio",
    "plugin",
    "registry",
    "search",
    "metadata",
    "vst",
    "vst3",
    "download"
  ],
  "author": "kmturley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/studiorack/studiorack-registry/issues"
  },
  "homepage": "https://github.com/studiorack/studiorack-registry#readme",
  "dependencies": {
    "@studiorack/core": "^1.2.5",
    "@types/semver": "^7.5.8",
    "graphql-request": "^6.1.0",
    "node-fetch": "^3.3.2",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/node-fetch": "^2.6.11",
    "prettier": "^3.2.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  }
}
